{
  "paths": {
    "input": "./input",
    "staging": "./staging",
    "output": "./output"
  },
  "rasterize": {
    "enabled": true,
    "concurrency": 2,
    "resolutions": [
      {
        "dpi": 100,
        "quality": 65,
        "lossless": false,
        "folder": "r100"
      },
      {
        "dpi": 300,
        "quality": 100,
        "lossless": true,
        "folder": "r300"
      }
    ]
  },
  "vlm": {
    "baseUrl": "http://127.0.0.1:1234/v1",
    "model": "qwen/qwen3-v1-2b",
    "timeout": 30000
  }
}
